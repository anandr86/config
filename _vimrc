set nocompatible
set backspace=indent,eol,start
filetype plugin indent on
syntax on
set smartindent
set autochdir
set number
set expandtab
set ts=4 sts=4 sw=4
set guifont="Source Code Pro"
set background=dark
let g:solarized_termtrans=1
colorscheme solarized
set guioptions-=m
set guioptions-=T
set guioptions-=r
set guioptions-=L
